{
    "name": "chatroom-react",
    "version": "1.0.0",
    "description": "Chatroom Application using Node and React",
    "main": "src/backend/server.js",
    "scripts": {
        "build": "webpack --mode production",
        "test": "export NODE_ENV=test && mocha src/backend/**/*.test.js",
        "start": "npm run build && node src/backend/server.js",
        "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
        "server": "nodemon src/backend/server.js",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "lint": "eslint src",
        "format": "eslint --fix src"
    },
    "author": "Szymon Ludyga",
    "license": "ISC",
    "dependencies": {
        "@material-ui/core": "^4.3.1",
        "@material-ui/icons": "^4.2.1",
        "axios": "^0.19.0",
        "babel-polyfill": "^6.26.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "classnames": "^2.2.6",
        "connected-react-router": "^6.5.2",
        "cors": "^2.8.5",
        "dotenv": "^8.1.0",
        "express": "^4.17.1",
        "history": "^4.9.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
        "mongodb": "^3.3.3",
        "mongoose": "^5.7.0",
        "morgan": "^1.9.1",
        "prop-types": "^15.7.2",
        "react": "^16.5.2",
        "react-dom": "^16.5.2",
        "react-redux": "^7.1.0",
        "react-router": "^5.0.1",
        "redux": "^4.0.4",
        "redux-devtools-extension": "^2.13.8",
        "redux-persist": "^6.0.0",
        "redux-thunk": "^2.3.0",
        "socket.io": "^2.2.0",
        "socket.io-client": "^2.2.0",
